{
  "extends": [
    "next/core-web-vitals",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "rules": {
    "no-console": [
      "error",
      {
        "allow": ["warn", "error"]
      }
    ],
    "import/no-unused-modules": [
      1,
      {
        "unusedExports": true,
        "missingExports": true
      }
    ],
    "import/named": "off"
  }
}
